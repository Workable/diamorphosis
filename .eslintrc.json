{
  "extends": ["workable-node"],
  "rules": {
    "operator-linebreak": "off",
    "security/detect-object-injection": "off",
    "security/detect-non-literal-require": "off",
    "security/detect-non-literal-fs-filename": "off",
    "no-process-env": "off",
    "no-console": "off",
    "no-sync": "off",
    "max-len": [
      "error",
      {
        "code": 120,
        "ignoreUrls": true
      }
    ]
  },
  "globals": {}
}
